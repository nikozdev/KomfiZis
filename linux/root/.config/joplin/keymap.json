[
    { "keys": [":"], "type": "function", "command": "enter_command_line_mode" },
    { "keys": ["h", "LEFT"], "type": "function", "command": "focus_previous" },
    { "keys": ["l", "RIGHT"], "type": "function", "command": "focus_next" },
    { "keys": ["k", "UP"], "type": "function", "command": "move_up" },
    { "keys": ["j", "DOWN"], "type": "function", "command": "move_down" },
    { "keys": ["K"], "type": "function", "command": "page_up" },
    { "keys": ["J"], "type": "function", "command": "page_down" },
    { "keys": ["ENTER"], "type": "function", "command": "activate" },
    { "keys": ["DELETE", "BACKSPACE"], "type": "function", "command": "delete" },
    { "keys": ["["], "type": "function", "command": "previous_link" },
    { "keys": ["]"], "type": "function", "command": "next_link" },
    { "keys": ["o"], "type": "function", "command": "open_link" },
    { "keys": ["tt"], "command": "todo toggle $n" },
